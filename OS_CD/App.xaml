<Application x:Class="OS_CD.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OS_CD">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Resources.xaml" />
                <ResourceDictionary Source="Themes/ColorConfig/DefaultTheme.xaml"/>
                <ResourceDictionary Source="Themes/CostumeStyles/SimpleStyles.xaml" />
                <ResourceDictionary Source="Themes/Generic.xaml" />
                <ResourceDictionary Source="Themes/CostumeStyles/ComplexStyles.xaml" />
                <ResourceDictionary Source="Themes/CostumeStyles/ControlResources.xaml" />
                <ResourceDictionary Source="Themes/CostumeStyles/DialogStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
